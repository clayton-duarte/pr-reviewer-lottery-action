{
  "extends": ["plugin:github/recommended"],
  "plugins": ["jest", "@typescript-eslint"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "rules": {
    "camelcase": "off"
  },
  "env": {
    "jest/globals": true,
    "node": true,
    "es6": true
  }
}
